{
  "name": "secondhandeshop",
  "version": "1.0.0",
  "description": "eCommerce application buil with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbArturas/secondhandshop.git"
  },
  "author": "Arturas Bieliunas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbArturas/secondhandshop/issues"
  },
  "homepage": "https://github.com/mbArturas/secondhandshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
